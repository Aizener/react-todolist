{
  "compilerOptions": {
    "target": "es5",
    "jsx": "preserve",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "include": ["./src", "typed-css.d.ts"]
}
